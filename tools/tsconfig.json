{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": false,
    "allowUnusedLabels": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "target": "es5",
    "lib": ["esnext"],
    "baseUrl": "./"
  }
}
